++ help="
++ Try \`$me --help' for more information."
++ 
++ # Parse command line
++ while test $# -gt 0 ; do
++   case $1 in
++     --time-stamp | --time* | -t )
++        echo "$timestamp" ; exit 0 ;;
++     --version | -v )
++        echo "$version" ; exit 0 ;;
++     --help | --h* | -h )
++        echo "$usage"; exit 0 ;;
++     -- )     # Stop option processing
++        shift; break ;;
++     - )	# Use stdin as input.
++        break ;;
++     -* )
++        echo "$me: invalid option $1$help" >&2
++        exit 1 ;;
++     * )
++        break ;;
++   esac
++ done
++ 
++ if test $# != 0; then
++   echo "$me: too many arguments$help" >&2
++   exit 1
++ fi
